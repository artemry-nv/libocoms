#ifndef __LIBSERVICE_VERSION_H__
#define __LIBSERVICE_VERSION_H__

#define LIBSERVICE_MINOR_BIT   (16UL)
#define LIBSERVICE_MAJOR_BIT   (24UL)
#define LIBSERVICE_VERNO_MICRO @MICRO_VERSION@
#define LIBSERVICE_API         (@MAJOR_VERSION@@MINOR_VERSION@)

#define LIBSERVICE_VERNO_STRING "@MAJOR_VERSION@.@MINOR_VERSION@.@MICRO_VERSION@"
#define LIBSERVICE_VER(major, minor) (((major)<<LIBSERVICE_MAJOR_BIT)|((minor)<<LIBSERVICE_MINOR_BIT))

#endif
